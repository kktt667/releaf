# Copy to .env and edit values:
#   cp .env.example .env

# App behavior
ENABLE_CHAIN=1
WALLET_ADDRESS=0xYourWalletAddressHere
EMERGENCY_MODE=1

# Thirdweb/API endpoints
THIRDWEB_MINT_URL=https://your-mint-endpoint.example.com
THIRDWEB_UPDATE_URL=https://your-update-endpoint.example.com
THIRDWEB_API_KEY=your_api_key_here
NFT_STORAGE_API_KEY=your_nft_storage_api_key_here

# Runtime options
CAMERA_INDEX=0
UPLOAD_PORT=8088
UPLOAD_HOST_IP=
TRAINING_FOLDER=photos
FLOWERS_FOLDER=flowers

# One-command runner behavior
RUN_OVERLAY=0
RESET_DEMO_STATE=1
